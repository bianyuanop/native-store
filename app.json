{
  "expo": {
    "name": "store",
    "slug": "store",
    "version": "1.0.0",
	"sdkVersion": "42.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
	  "bundleIdentifier":"com.qianiqi.store",
	  "buildNumber": "1.0.0"
    },
    "android": {
		"package":"com.qianiqi.store",
		"versionCode": 1,
		"adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
